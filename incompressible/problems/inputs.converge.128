# simple inputs files for the incompressible convergence test

[driver]
max_steps = 1000
tmax = 0.5
cfl = 0.5
init_tstep_factor = 1.0
fix_dt = 1.25e-3


[incompressible]
num_init_proj = 0


[io]
basename = converge_128_
n_out = 20


[mesh]
nx = 128
ny = 128
xmax = 1.0
ymax = 1.0

xlboundary = periodic
xrboundary = periodic

ylboundary = periodic
yrboundary = periodic


[converge]



